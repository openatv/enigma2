		<item level="2" text="Hide zap errors">config.usage.hide_zap_errors</item>
		<item level="2" text="Hide CI messages">config.usage.hide_ci_messages</item>
	</setup>
	<setup key="epgsetup" title="Settings...">
		<item level="2" text="EPG location">config.misc.epgcachepath</item>
		<item level="2" text="EPG filename">config.misc.epgcachefilename</item>
		<item level="2" text="Automatic refresh">config.epg.cacheloadsched</item>
		<item level="2" text="Refresh every (in hours)" requires="config.epg.cacheloadsched">config.epg.cacheloadtimer</item>
		<item level="2" text="Automatic save">config.epg.cachesavesched</item>
		<item level="2" text="Save every (in hours)" requires="config.epg.cachesavesched">config.epg.cachesavetimer</item>
		<item level="1" text="Show EIT now/next in infobar">config.usage.show_eit_nownext</item>
