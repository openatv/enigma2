		<item level="1" text="Enable MHW EPG">config.epg.mhw</item>
		<item level="1" text="Enable freesat EPG">config.epg.freesat</item>
		<item level="1" text="Enable ViaSat EPG">config.epg.viasat</item>
		<item level="1" text="Enable Netmed EPG">config.epg.netmed</item>
		<item level="2" text="Maintain old EPG data for">config.epg.histminutes</item>
